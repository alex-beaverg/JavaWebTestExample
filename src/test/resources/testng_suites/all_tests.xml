<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Test Site web testing" verbose="1" parallel="tests" thread-count="1">
    <test name="Main Menu testing" parallel="methods" thread-count="1">
        <classes>
            <class name="com.beaverg.tests.MainMenuTest"/>
        </classes>
    </test>
    <test name="Product testing" parallel="methods" thread-count="1">
        <classes>
            <class name="com.beaverg.tests.ProductTest"/>
        </classes>
    </test>
    <test name="Logining testing" parallel="methods" thread-count="1">
        <classes>
            <class name="com.beaverg.tests.LoginTest"/>
        </classes>
    </test>
</suite>